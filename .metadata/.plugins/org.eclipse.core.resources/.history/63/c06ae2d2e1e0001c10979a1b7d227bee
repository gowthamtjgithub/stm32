/*
 * PB14 --> SPI2_MISO
 * PB15 -->  SPI2_MOSI
 * PB12 -->  SPI2_SCLK
 * PB12 --> SPI2_NSS
 * ALF function mode : 5
 */

#include "stm32f4xx.h"
#include "stm32f407xx_gpio_driver.h"

int main(void)
{
	//this function is used to initialize the GPIO pins to behave as SPI2 pins
	SPI_GPIOInits();
	return 0;
}
